<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 아래와 같이 namespace를 적으면, 여기에서 작성된 쿼리는 저기 TimeMapper 인터페이스와 바인딩이 되는 것이다 -->
<mapper namespace="com.example.ex02.mapper.TestMapper">
<!-- config.xml에서 alias 설정 했음 -->
	<select id="getList" resultType="testVO"> 
		SELECT ID, NAME FROM TBL_TEST
	</select>
</mapper>